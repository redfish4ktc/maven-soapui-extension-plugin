<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>soapui.extension</groupId>
    <artifactId>common-it-parent</artifactId>
    <version>@project.version@</version>
    <relativePath>../common.xml</relativePath>
  </parent>
  <groupId>soapui.extension</groupId>
  <artifactId>test-multi-goal</artifactId>
  <packaging>pom</packaging>
  <version>@project.version@</version>
  
  <properties>
    <soapui.configuration.path>${basedir}/../../test-verify_goal/soapui-conf</soapui.configuration.path>
    <soapui.configuration.projects.path>${soapui.configuration.path}/projects</soapui.configuration.projects.path>
    <soapui.configuration.projects.alternate.path>${soapui.configuration.path}/projects-alternate</soapui.configuration.projects.alternate.path>
    <soapui.project.success.1.file>simple-sucess-1-soapui-project.xml</soapui.project.success.1.file>
    <soapui.project.success.2.file>simple-sucess-2-soapui-project.xml</soapui.project.success.2.file>
    <soapui.project.fail.1.file>simple-fail-1-soapui-project.xml</soapui.project.fail.1.file>
    <soapui.project.fail.2.file>simple-fail-2-soapui-project.xml</soapui.project.fail.2.file>
    <soapui.composite.project.success.1.directory>simple-composite-sucess-1</soapui.composite.project.success.1.directory>
    <soapui.settings.empty.file>${soapui.configuration}/soapui-settings/empty-soapui-settings.xml</soapui.settings.empty.file>
    <soapui.config.output.directory>${project.build.directory}/soapui/ouput</soapui.config.output.directory>
  </properties>

</project>
